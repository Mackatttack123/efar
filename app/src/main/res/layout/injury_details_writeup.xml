<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="369dp"
        android:layout_height="700dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:background="#eeeeee"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="31dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:background="#aaaaaa"
        android:text="Box 3: Injury Details"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Box 3: Injury Details"/>

    <TextView
        android:id="@+id/subTitleTextView1"
        android:layout_width="301dp"
        android:layout_height="23dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="#ddddff"
        android:text="Violent Injury"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="301dp"
        android:layout_height="23dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="#ddddff"
        android:text="Motor Vehicle Accident"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="301dp"
        android:layout_height="23dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="7dp"
        android:background="#ddddff"
        android:text="Medical Emergency"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText2"/>

    <TextView
        android:id="@+id/textView9"
        android:layout_width="301dp"
        android:layout_height="23dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="#ddddff"
        android:text="Injuries And Illness"
        android:textAlignment="center"
        android:textSize="18sp"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText5"/>

    <TextView
        android:id="@+id/weaponUsedTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Weapon Used:"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/subTitleTextView1"
        android:layout_marginStart="8dp"/>

    <RadioGroup
        android:id="@+id/radioGroup"
        android:orientation='horizontal'
        android:layout_width="302dp"
        android:layout_height="28dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weaponUsedTextView">

        <RadioButton
            android:id="@+id/radioButton"
            android:layout_width="101dp"
            android:layout_height="29dp"
            android:layout_weight="1"
            android:text="No Wepon"
            tools:layout_editor_absoluteX="0dp"
            tools:layout_editor_absoluteY="0dp"/>

        <RadioButton
            android:id="@+id/radioButton3"
            android:layout_width="74dp"
            android:layout_height="28dp"
            android:layout_weight="1"
            android:text="Knife"
            tools:layout_editor_absoluteX="182dp"
            tools:layout_editor_absoluteY="111dp"/>

        <RadioButton
            android:id="@+id/radioButton2"
            android:layout_width="78dp"
            android:layout_height="26dp"
            android:layout_weight="1"
            android:text="Gun"
            tools:layout_editor_absoluteX="117dp"
            tools:layout_editor_absoluteY="111dp"/>
    </RadioGroup>

    <EditText
        android:id="@+id/editText"
        android:layout_width="215dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Other..."
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup"/>

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation='horizontal'
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/radioGroup2">

        <RadioButton
            android:id="@+id/radioButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Pedestrian"
            tools:layout_editor_absoluteX="114dp"
            tools:layout_editor_absoluteY="225dp"/>

        <RadioButton
            android:id="@+id/radioButton4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Passenger"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="225dp"/>
    </RadioGroup>

    <EditText
        android:id="@+id/editText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Other..."
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup2"/>

    <CheckBox
        android:id="@+id/checkBox3"
        android:layout_width="96dp"
        android:layout_height="31dp"
        android:layout_marginTop="8dp"
        android:text="Poisoning"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"/>

    <CheckBox
        android:id="@+id/checkBox4"
        android:layout_width="96dp"
        android:layout_height="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="Epilepsy"
        app:layout_constraintLeft_toRightOf="@+id/checkBox7"
        app:layout_constraintTop_toBottomOf="@+id/checkBox6"/>

    <CheckBox
        android:id="@+id/checkBox10"
        android:layout_width="107dp"
        android:layout_height="42dp"
        android:text="Abdominal Pain"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox8"
        android:layout_marginStart="8dp"/>

    <CheckBox
        android:id="@+id/checkBox8"
        android:layout_width="96dp"
        android:layout_height="0dp"
        android:text="Stroke"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox3"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"/>

    <CheckBox
        android:id="@+id/checkBox11"
        android:layout_width="119dp"
        android:layout_height="31dp"
        android:text="Unconscious"
        app:layout_constraintLeft_toRightOf="@+id/checkBox12"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        android:layout_marginStart="8dp"/>

    <CheckBox
        android:id="@+id/checkBox16"
        android:layout_width="96dp"
        android:layout_height="0dp"
        android:text="Choking"
        app:layout_constraintLeft_toRightOf="@+id/checkBox11"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        android:layout_marginStart="8dp"/>

    <CheckBox
        android:id="@+id/checkBox15"
        android:layout_width="96dp"
        android:layout_height="0dp"
        android:text="Fracture"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox12"
        app:layout_constraintLeft_toRightOf="@+id/checkBox14"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <CheckBox
        android:id="@+id/checkBox14"
        android:layout_width="96dp"
        android:layout_height="0dp"
        android:text="Bleeding"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox12"
        android:layout_marginStart="8dp"/>

    <CheckBox
        android:id="@+id/checkBox13"
        android:layout_width="96dp"
        android:layout_height="0dp"
        android:text="Burns"
        app:layout_constraintLeft_toRightOf="@+id/checkBox15"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox12"
        android:layout_marginStart="8dp"/>

    <CheckBox
        android:id="@+id/checkBox12"
        android:layout_width="80dp"
        android:layout_height="31dp"
        android:text="Awake"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        android:layout_marginStart="8dp"/>

    <CheckBox
        android:id="@+id/checkBox7"
        android:layout_width="96dp"
        android:layout_height="0dp"
        android:text="Difficulty Breathing"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox5"
        app:layout_constraintLeft_toRightOf="@+id/checkBox8"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <CheckBox
        android:id="@+id/checkBox6"
        android:layout_width="96dp"
        android:layout_height="33dp"
        android:layout_marginTop="8dp"
        android:text="Diabetes"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintLeft_toRightOf="@+id/checkBox5"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <CheckBox
        android:id="@+id/checkBox5"
        android:layout_width="96dp"
        android:layout_height="33dp"
        android:layout_marginTop="8dp"
        android:text="Chest Pain"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintLeft_toRightOf="@+id/checkBox3"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <EditText
        android:id="@+id/editText6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Other..."
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkBox14"/>

    <EditText
        android:id="@+id/editText5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Other..."
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/checkBox10"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"/>

</android.support.constraint.ConstraintLayout>